{
  "name": "@super-protocol/sdk-js",
  "version": "2.2.0-beta.38",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/mjs/index.js"
  },
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest -i",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "build:cjs": "tsc -p ./tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "build:mjs": "tsc -p ./tsconfig.mjs.json && echo '{\"type\": \"module\"}' > dist/mjs/package.json",
    "build": "rm -rf ./build && npm run build:cjs && npm run build:mjs",
    "prepare": "npm run build",
    "docs": "typedoc src/index.ts --excludePrivate --plugin typedoc-plugin-markdown --out ./auto-docs",
    "docs-cleanup": "rm -rf auto-docs",
    "bump": "npm version prerelease --preid=beta && npm run tag:publish",
    "patch:beta": "npm version prepatch --preid=beta && npm run tag:publish",
    "minor:beta": "npm version preminor --preid=beta && npm run tag:publish",
    "major:beta": "npm version premajor --preid=beta && npm run tag:publish",
    "patch": "npm version patch && npm run tag:publish",
    "minor": "npm version minor && npm run tag:publish",
    "major": "npm version major && npm run tag:publish",
    "tag:publish": "git push origin && git push origin $(git tag --points-at HEAD)",
    "dto:update": "npm run dto:clean && npm run dto:generate",
    "dto:clean": "rm -rf ./src/proto/*",
    "dto:generate": "cd ./dto/src && protoc --plugin=../../node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_opt=importSuffix=.js --ts_proto_opt=unrecognizedEnum=false --ts_proto_out=../../src/proto ./*"
  },
  "devDependencies": {
    "@super-protocol/eslint-config-typescript": "2.0.0",
    "@types/elliptic": "^6.4.18",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.176",
    "@types/lru-cache": "^7.10.10",
    "@types/node": "^16.18.59",
    "@types/node-forge": "^1.3.1",
    "@types/node-gzip": "^1.1.0",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "net": "^1.0.2",
    "ts-jest": "^29.1.2",
    "ts-proto": "^1.110.4",
    "ts-protoc-gen": "^0.15.0",
    "typedoc": "^0.25.9",
    "typedoc-plugin-markdown": "^3.17.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.470.0",
    "@fidm/x509": "^1.2.1",
    "@super-protocol/dto-js": "0.0.36",
    "@super-protocol/tee-lib": "^0.1.2",
    "@super-protocol/uplink-nodejs": "1.2.19",
    "asn1js": "^3.0.5",
    "axios": "^1.5.1",
    "bottleneck": "^2.19.5",
    "elliptic": "^6.5.5",
    "ethers": "^5.5.3",
    "js-encoding-utils": "^0.7.3",
    "lodash": "^4.17.21",
    "lru-cache": "^10.0.0",
    "node-forge": "^1.3.1",
    "node-gzip": "^1.1.2",
    "p-queue": "6.6.2",
    "pino": "^7.2.0",
    "pkijs": "^3.0.15",
    "protobufjs": "^6.11.2",
    "ua-parser-js": "^1.0.37",
    "uuid": "^9.0.1",
    "web3": "4.7.0",
    "web3-core-requestmanager": "^1.10.0",
    "web3-eth-abi": "^4.1.2",
    "web3-types": "^1.1.0"
  },
  "repository": "git://github.com/Super-Protocol/sdk-js.git",
  "resolutions": {
    "wrap-ansi": "7.0.0"
  }
}
